package com.yutong.axxc.tqc.entity.oauth;

import javax.xml.bind.annotation.XmlElement;

import com.fasterxml.jackson.annotation.JsonIgnore;


/**
 * 用户基本信息.
 * 
 
 * @version $Revision 1.0 $ 2013-3-17 下午12:54:17
 */
public class UsrInfo {

	/**
	 * 用户ID.
	 */
	@XmlElement(name = "usr_id")
	private String usrId;

	/**
	 * 用户尊称.
	 */
	@XmlElement(name = "usr_alias")
	private String usrAlias;

	/**
	 * 用户性别.
	 */
	@XmlElement(name = "usr_sex")
	private String usrSex;

	/**
	 * 企业ID.
	 */
	@XmlElement(name = "en_id")
	private String enId;

	/**
	 * 组织ID.
	 */
	@XmlElement(name = "org_id")
	private String orgId;

	/**
	 * 用户所属企业名称.
	 */
	@XmlElement(name = "usr_en_name")
	private String usrEnName;

	/**
	 * 企业所在省份.
	 */
	@XmlElement(name = "en_prov")
	private String enProv;

	/**
	 * 企业所在地市.
	 */
	@XmlElement(name = "en_city")
	private String enCity;

	/**
	 * 用户信息ETag值.
	 */
	@XmlElement(name = "ETag")
	private String usrInfoETag;
	
	/**
	 * 终端用户授权信息.
	 */
	@JsonIgnore
	private String accessToken;
	
	/**
	 * 该企业下用户是否允许使用移动终端软件.1:允许;0:不允许.
	 */
	@JsonIgnore
	private String isMobileAllow;

	/**
	 * @return Returns the usrId.
	 */
	public String getUsrId() {
		return usrId;
	}

	/**
	 * @param usrId
	 *            The usrId to set.
	 */
	public void setUsrId(String usrId) {
		this.usrId = usrId;
	}

	/**
	 * @return Returns the usrAlias.
	 */
	public String getUsrAlias() {
		return usrAlias;
	}

	/**
	 * @param usrAlias
	 *            The usrAlias to set.
	 */
	public void setUsrAlias(String usrAlias) {
		this.usrAlias = usrAlias;
	}

	/**
	 * @return Returns the usrInfoETag.
	 */
	public String getUsrInfoETag() {
		return usrInfoETag;
	}

	/**
	 * @param usrInfoETag The usrInfoETag to set.
	 */
	public void setUsrInfoETag(String usrInfoETag) {
		this.usrInfoETag = usrInfoETag;
	}

	/**
	 * @return Returns the usrEnName.
	 */
	public String getUsrEnName() {
		return usrEnName;
	}

	/**
	 * @param usrEnName
	 *            The usrEnName to set.
	 */
	public void setUsrEnName(String usrEnName) {
		this.usrEnName = usrEnName;
	}

	/**
	 * @return Returns the usrSex.
	 */
	public String getUsrSex() {
		return usrSex;
	}

	/**
	 * @param usrSex
	 *            The usrSex to set.
	 */
	public void setUsrSex(String usrSex) {
		this.usrSex = usrSex;
	}

	/**
	 * @return Returns the enId.
	 */
	public String getEnId() {
		return enId;
	}

	/**
	 * @param enId The enId to set.
	 */
	public void setEnId(String enId) {
		this.enId = enId;
	}

	/**
	 * @return Returns the accessToken.
	 */
	public String getAccessToken() {
		return accessToken;
	}

	/**
	 * @param accessToken The accessToken to set.
	 */
	public void setAccessToken(String accessToken) {
		this.accessToken = accessToken;
	}

	/**
	 * @return Returns the enProv.
	 */
	public String getEnProv() {
		return enProv;
	}

	/**
	 * @param enProv The enProv to set.
	 */
	public void setEnProv(String enProv) {
		this.enProv = enProv;
	}

	/**
	 * @return Returns the enCity.
	 */
	public String getEnCity() {
		return enCity;
	}

	/**
	 * @param enCity The enCity to set.
	 */
	public void setEnCity(String enCity) {
		this.enCity = enCity;
	}

	/**
	 * @return Returns the isMobileAllow.
	 */
	public String getIsMobileAllow() {
		return isMobileAllow;
	}

	/**
	 * @param isMobileAllow The isMobileAllow to set.
	 */
	public void setIsMobileAllow(String isMobileAllow) {
		this.isMobileAllow = isMobileAllow;
	}

	/**
	 * @return Returns the orgId.
	 */
	public String getOrgId() {
		return orgId;
	}

	/**
	 * @param orgId The orgId to set.
	 */
	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}

}
